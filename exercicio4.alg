algoritmo exercicio4
real areaBase , volume, raio, altura
início 
    escreva("Digite o valor do raio: ")
    leia(raio)
    escreva("Digite o valor da altura: ")
    leia(altura)
    areaBase = 3,14 * raio * raio
    volume = altura * areaBase
    escreva("A área da base do cilindro é: " + areaBase +)
    escreva("A volume do cilindro  é: " + volume +)
fim    
