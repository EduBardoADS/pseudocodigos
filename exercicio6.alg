algoritmo "exercicio6"
      real sol, passaros, hipotenusa
      inicio
        escreva ( "digite quantos metrosn de cerca tem  a rua do sol" )
        leia (sol)
		escreva ( "digite quantos metros tem a cerca da estrada dos passaros" )
		leia (passaros)
		hipotenusa = raiz^2((sol^2 + passaros^2))
		escreva ( "Falta " hipotenusa , "metros de cerca")
		fim
